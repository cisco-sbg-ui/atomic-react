{
  "name": "@cisco-sbg-ui/atomic-react",
  "version": "3.3.0",
  "description": "",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "scripts": {
    "build:babel-preset": "node build/generate-babel-preset.js",
    "build:lib": "babel --no-babelrc --config-file ./babel-lib.config.js framework --out-dir lib --copy-files",
    "build:docs": "next build",
    "build": "npm run build:lib && npm run build:babel-preset",
    "vercel-build": "npm run build:docs",
    "coverage-report": "npx nyc report --reporter=text-summary",
    "cypress:install": "./node_modules/.bin/cypress install",
    "cypress:open": "cypress open --env snapshots=off",
    "cypress:run": "cypress run --env type=actual --browser chrome",
    "cypress:runUpdateSnapshots": "cypress run --env type=base --browser chrome",
    "dev": "next",
    "esbuild:install": "node ./node_modules/esbuild/install.js",
    "publish-package": "rimraf lib && npm ci && npm run build && npm publish",
    "start": "next start",
    "test:dev": "cross-env WAIT_ON_TIMEOUT=600000 start-server-and-test dev http://localhost:3000 cypress:open",
    "test:lint": "eslint cypress docs framework pages --max-warnings 0",
    "test:components": "cross-env WAIT_ON_TIMEOUT=600000 INSTRUMENT=true start-server-and-test dev http://localhost:3000 cypress:run",
    "test": "npm run test:lint && npm run test:components",
    "updateSnapshots": "start-server-and-test dev http://localhost:3000 cypress:runUpdateSnapshots",
    "upload-coverage": "codecov"
  },
  "peerDependencies": {
    "prop-types": "^15.0.0",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "dependencies": {
    "babel-plugin-auto-import": "1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.16.8",
    "@babel/core": "7.17.4",
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@cypress/code-coverage": "3.9.12",
    "algoliasearch": "4.12.0",
    "babel-plugin-istanbul": "6.1.1",
    "codecov": "3.8.3",
    "cross-env": "7.0.3",
    "cypress": "9.4.1",
    "cypress-plugin-tab": "1.0.5",
    "cypress-visual-regression": "1.6.3",
    "cypress-wait-until": "1.7.2",
    "dotenv": "15.0.0",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-anti-trojan-source": "1.1.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "glob": "7.2.0",
    "gray-matter": "4.0.3",
    "lodash.debounce": "4.0.8",
    "next": "12.0.10",
    "next-mdx-remote": "3.0.8",
    "prettier": "2.5.1",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-docgen": "5.4.0",
    "react-dom": "17.0.2",
    "react-live": "2.2.3",
    "remark-slug": "6.1.0",
    "remove-markdown": "0.3.0",
    "rimraf": "3.0.2",
    "sass": "1.49.0",
    "start-server-and-test": "1.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cisco-sbg-ui/atomic-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cisco-sbg-ui/atomic-react/issues"
  },
  "homepage": "https://atomic-react.security.cisco.com",
  "sideEffects": [
    "**/*.scss"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/cisco-sbg-ui"
  }
}
