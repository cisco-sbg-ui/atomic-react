{
  "name": "@cisco-ats/atomic-react",
  "version": "2.5.7",
  "description": "",
  "main": "index.js",
  "module": "lib/index.js",
  "scripts": {
    "build:babel-preset": "node build/generate-babel-preset.js",
    "build:lib": "cross-env NODE_ENV=lib babel framework --out-dir lib",
    "build:cra": "rollup -c ./rollup.config.js",
    "build:storybook": "build-storybook -s .storybook/public -c .storybook -o docs --docs",
    "build": "npm run build:cra && npm run build:lib && npm run build:babel-preset && npm run build:storybook",
    "coverage-report": "npx nyc report --reporter=text-summary",
    "cypress:install": "./node_modules/.bin/cypress install",
    "cypress:open": "cypress open --env snapshots=off",
    "cypress:run": "cypress run",
    "cypress:runUpdateSnapshots": "cypress run --env updateSnapshots=true",
    "cypress:lib": "cypress run --config integrationFolder=lib-demo",
    "dev": "start-server-and-test start:ci http://localhost:8081 cypress:open",
    "dev:lib": "webpack serve --config webpack.lib-demo.config.js",
    "start": "npm run build:cra && npm run build:lib && npm run build:babel-preset && start-storybook -p 8081 -s .storybook/public --docs",
    "start:ci": "npm run build:cra && npm run build:lib && npm run build:babel-preset && start-storybook -p 8081 -s .storybook/public --docs --ci",
    "test:components": "eslint framework .storybook --max-warnings 0 && cross-env INSTRUMENT=1 start-server-and-test start:ci http://localhost:8081 cypress:run",
    "test:lib": "start-server-and-test dev:lib http://localhost:8080 cypress:lib",
    "test": "npm run test:components && npm run test:lib",
    "updateSnapshots": "start-server-and-test start:ci http://localhost:8081 cypress:runUpdateSnapshots"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "16.x",
    "prop-types": "15.x"
  },
  "dependencies": {
    "babel-plugin-auto-import": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@cypress/code-coverage": "3.8.3",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@storybook/addon-docs": "6.0.26",
    "@storybook/react": "6.0.26",
    "algoliasearch": "^4.5.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-detective": "^2.0.0",
    "babel-plugin-istanbul": "6.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "5.0.0",
    "cypress": "5.3.0",
    "cypress-image-snapshot": "^3.1.1",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-wait-until": "^1.7.1",
    "eslint": "7.12.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob": "^7.1.6",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "4.5.0",
    "lodash.debounce": "^4.0.8",
    "prettier": "2.1.2",
    "react-is": "^16.13.1",
    "rollup": "2.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "3.1.8",
    "sass": "^1.27.0",
    "sass-loader": "10.0.4",
    "start-server-and-test": "^1.11.5",
    "style-loader": "^2.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/threatgrid/atomic-react.git"
  },
