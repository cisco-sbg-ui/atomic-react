{
  "name": "@cisco-ats/atomic-react",
  "version": "2.13.3",
  "description": "",
  "main": "index.js",
  "module": "lib/index.js",
  "scripts": {
    "build:babel-preset": "node build/generate-babel-preset.js",
    "build:lib": "cross-env BABEL_ENV=lib babel framework --out-dir lib",
    "build:cra": "cross-env BABEL_ENV=cra rollup -c ./rollup.config.js",
    "build:docs": "gatsby build",
    "build": "npm run build:cra && npm run build:lib && npm run build:babel-preset",
    "vercel-build": "npm run build:docs",
    "coverage-report": "npx nyc report --reporter=text-summary",
    "cypress:install": "./node_modules/.bin/cypress install",
    "cypress:open": "cypress open --env snapshots=off",
    "cypress:run": "cypress run",
    "cypress:runUpdateSnapshots": "cypress run --env env=\"{'env': {'cypress-plugin-snapshots':{'updateSnapshots':true}}}\"",
    "cypress:lib": "cypress run --config integrationFolder=lib-demo",
    "dev": "cross-env WAIT_ON_TIMEOUT=600000 start-server-and-test start http://localhost:8081 cypress:open",
    "dev:lib": "cross-env BABEL_ENV=lib-demo webpack serve --config webpack.lib-demo.config.js",
    "start": "gatsby develop -p 8081",
    "test:lint": "eslint framework --max-warnings 0",
    "test:components": "cross-env WAIT_ON_TIMEOUT=600000 INSTRUMENT=true start-server-and-test start http://localhost:8081 cypress:run",
    "test:lib": "npm run build && start-server-and-test dev:lib http://localhost:8080 cypress:lib",
    "test": "npm run test:lint && npm run test:components && npm run test:lib",
    "updateSnapshots": "start-server-and-test start http://localhost:8081 cypress:runUpdateSnapshots",
    "upload-coverage": "codecov"
  },
  "peerDependencies": {
    "prop-types": "^15.0.0",
